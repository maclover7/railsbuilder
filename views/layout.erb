    <style>
      /* Status Lights Styling: */

      /* Boilerplate: */
      #light {
        overflow: hidden;
        padding: 0 0 4px;
        width: 30px;
      }

      #light span {
        border-radius: 40px;
        display: block;
        height: 20px;
        margin: 2px auto 0;
        opacity: .5;
        width: 20px;
      }

      /* Light Colors: */
      #red    { background-color: red; }
      #green  { background-color: green; }
      #black    { background-color: black; }
      #light span.active    { opacity: 1; }

      .center {
        text-align: center;
      }
    </style>

